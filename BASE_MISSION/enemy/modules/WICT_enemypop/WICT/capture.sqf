/* Initialize these variables (so you don't get format 'any') */_message = "none";_flag = "none";/* Check ONLY on the server... */if (isServer) then{		private ["_call","_foundW","_foundE","_foundN"];	_call = _this select 0;	_flag = _this select 1;	/* Searching for the flag in all lists */	_foundW = 0;	_foundE = 0;	_foundN = 0;		/* Different situations - who is capturing what? */		{			if (_x == _flag) then {_foundE = 1};		} forEach WICT_ebl;		{			if (_x == _flag) then {_foundW = 1};		} forEach WICT_wbl;		{			if (_x == _flag) then {_foundN = 1};		} forEach WICT_nbl;	/* See messages... */	if ((_foundE > 0) and (_call == "west")) then	{		WICT_ebl = WICT_ebl - [_flag];		WICT_wbl = WICT_wbl + [_flag];		_message = "BO";	};	if ((_foundW > 0) and (_call == "east")) then	{		WICT_wbl = WICT_wbl - [_flag];		WICT_ebl = WICT_ebl + [_flag];		_message = "OB";			};	if ((_foundN > 0) and (_call == "west")) then	{		WICT_nbl = WICT_nbl - [_flag];		WICT_wbl = WICT_wbl + [_flag];		_message = "BN";		};	if ((_foundN > 0) and (_call == "east")) then	{		WICT_nbl = WICT_nbl - [_flag];		WICT_ebl = WICT_ebl + [_flag];			};	if ((_call == "neutral") and (_foundE == 1)) then	{		WICT_ebl = WICT_ebl - [_flag];		WICT_nbl = WICT_nbl + [_flag];		_message = "NO";	};	if ((_call == "neutral") and (_foundW == 1)) then	{		WICT_wbl = WICT_wbl - [_flag];		WICT_nbl = WICT_nbl + [_flag];		_message = "NB";	};};/* ...but send message to all !!! */if (_message != "none") then{	[nil,nil,rEXECVM,(WICT_PATH + "WICT_data\captureMessage.sqf"),_message,_flag] call RE;};